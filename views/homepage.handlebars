{{#each articles as |article| }}
<div class="row align-center">
  <div class="col-md-6">
    <h2>{{article.title}}</h2>
  </div>
  <div class="col-md-6">
    <h4>{{article.author}}</h4>
  </div>
  <div class="container-fluid">
    <p class="col-md-5">{{article.content}}</p>
  </div>
</div>
{{/each}}